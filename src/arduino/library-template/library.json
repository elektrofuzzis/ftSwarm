{
    "name": "ftSwarm",
    "description": "Interface libray to build robotics swarms using ftSwarm/ESP32 hardware",
    "keywords": "ftSwarm,swarm,robotics,fischertechnik,esp32",
    "authors": [
        {
            "name": "Christian Bergschneider",
            "url": "https://github.com/Bloeckchengrafik",
            "maintainer": true
        },
        {
            "name": "Stefan Fuss",
            "url": "https://github.com/yet-another-fuzzi",
            "maintainer": true
        }
    ],
    "depends": "Adafruit GFX Library, Adafruit SSD1306, FastLED, STM32duino LSM6DSR",
    "repository": {
        "type": "git",
        "url": "https://github.com/elektrofuzzis/ftSwarm"
    },
    "version": "0.6.2",
    "license": "MIT",
    "homepage": "https://elektrofuzzis.githib.io/ftSwarm",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "build": {
        "srcFilter": [
            "+<*.c>",
            "+<*.cpp>",
            "+<*.h>",
            "+<platforms/esp/32/*.cpp>"
        ],
        "libArchive": false
    }
}